<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="508.0" prefWidth="576.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="main.view.controller.AnalysisController">
    <effect>
        <DropShadow/>
    </effect>
    <children>
        <AnchorPane layoutY="-9.0" prefHeight="517.0" prefWidth="608.0"
                    style="-fx-border-color: #130000; -fx-border-width: 4;">
            <children>
                <AnchorPane layoutX="15.0" layoutY="9.0" prefHeight="66.0" prefWidth="611.0"
                            style="-fx-background-color: #130000;" AnchorPane.leftAnchor="0.0"
                            AnchorPane.rightAnchor="0.0">
                    <children>
                        <Label layoutX="223.0" layoutY="12.0" text="DECISION" textFill="WHITE">
                            <font>
                                <Font name="Arial Rounded MT Bold" size="36.0"/>
                            </font>
                        </Label>
                    </children>
                    <effect>
                        <DropShadow/>
                    </effect>
                </AnchorPane>
                <JFXTextField fx:id="theMostCostEffectiveOptionTextField" editable="false" focusColor="#130000"
                              labelFloat="true" layoutX="53.0" layoutY="139.0" prefHeight="50.0"
                              promptText="The Most Cost Effective" AnchorPane.leftAnchor="50.0"
                              AnchorPane.rightAnchor="50.0">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </JFXTextField>
                <JFXButton buttonType="RAISED" layoutX="62.0" layoutY="192.0" onAction="#findTheMostCostEffectiveOption"
                           prefHeight="50.0" style="-fx-background-color: #074d07;" text="FIND" textFill="WHITE"
                           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </JFXButton>
                <JFXTextField fx:id="theMostBeneficialOptionTextField" editable="false" focusColor="#c35c07"
                              labelFloat="true" layoutX="54.0" layoutY="307.0" prefHeight="50.0"
                              promptText="The Most Beneficial" AnchorPane.leftAnchor="50.0"
                              AnchorPane.rightAnchor="50.0">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </JFXTextField>
                <JFXButton buttonType="RAISED" layoutX="62.0" layoutY="360.0" onAction="#findTheMostBeneficialOption"
                           prefHeight="50.0" style="-fx-background-color: #074d07;" text="FIND" textFill="WHITE"
                           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </JFXButton>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
